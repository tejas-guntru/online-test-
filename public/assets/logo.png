<svg
  width="600"
  height="200"
  viewBox="0 0 600 200"
  xmlns="http://www.w3.org/2000/svg"
>
  <!-- Outer emblem -->
  <circle
    cx="300"
    cy="100"
    r="80"
    fill="none"
    stroke="#C9A24D"
    stroke-width="6"
  />

  <!-- Inner emblem -->
  <circle
    cx="300"
    cy="100"
    r="60"
    fill="none"
    stroke="#E0C77A"
    stroke-width="2"
  />

  <!-- Brand name -->
  <text
    x="300"
    y="110"
    text-anchor="middle"
    font-family="Times New Roman, serif"
    font-size="42"
    font-weight="bold"
    fill="#1A1A1A"
  >
    TERIGO
  </text>

  <!-- Tagline -->
  <text
    x="300"
    y="145"
    text-anchor="middle"
    font-family="Times New Roman, serif"
    font-size="14"
    letter-spacing="2"
    fill="#555555"
  >
    CERTIFICATION AUTHORITY
  </text>
</svg>
